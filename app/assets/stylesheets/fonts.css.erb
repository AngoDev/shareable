@font-face {
  font-family: 'Glyphicons Halflings';
  src: url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.eot') %>");
  src: url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.eot') %>?#iefix") format('embedded-opentype'),
    url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.woff') %>") format('woff'),
    url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.woff2') %>") format('woff2'),
    url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.ttf') %>") format('truetype'),
    url("<%= asset_path('Bootstrap/glyphicons-halflings-regular.svg') %>#glyphicons_halflingsregular") format('svg');
}

@font-face {
  font-family: 'FontAwesome';
  src: url("<%= asset_path('Font-Awesome/fontawesome-webfont.eot') %>?v=4.3.0");
  src: url("<%= asset_path('Font-Awesome/fontawesome-webfont.eot') %>?#iefix&v=4.3.0") format('embedded-opentype'),
  url("<%= asset_path('Font-Awesome/fontawesome-webfont.woff2') %>?v=4.3.0") format('woff2'),
  url("<%= asset_path('Font-Awesome/fontawesome-webfont.woff') %>?v=4.3.0") format('woff'),
  url("<%= asset_path('Font-Awesome/fontawesome-webfont.ttf') %>?v=4.3.0") format('truetype'),
  url("<%= asset_path('Font-Awesome/fontawesome-webfont.svg') %>?v=4.3.0#fontawesomeregular") format('svg');
  font-weight: normal;
  font-style: normal;
}
